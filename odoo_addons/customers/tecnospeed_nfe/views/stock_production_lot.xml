<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="tecnospeed_stock_production_form" model="ir.ui.view">
        <field name="name">tecnospeed.stock.production.form</field>
        <field name="model">stock.production.lot</field>
        <field name="inherit_id" ref="product_expiry.view_move_form_expiry" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='removal_date']" position="after">
                <field name = "manufacture_date"/>
            </xpath>
        </field>
    </record>
</odoo>
