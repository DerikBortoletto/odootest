<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_lot_move_form_manufacture_date" model="ir.ui.view">
        <field name="name">stock.production.lot.extension.move.form.production</field>
        <field name="model">stock.production.lot</field>
        <field name="inherit_id" ref="product_expiry.view_move_form_expiry" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='use_date']" position="before">
                <field name="manufacture_date" />
            </xpath>
        </field>
    </record>

</odoo>
